# Backend API Configuration
REACT_APP_BACKEND_URL=https://fix-render-loop.preview.emergentagent.com
WDS_SOCKET_PORT=443

# Google OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your-google-oauth-client-id

# Cloudinary Configuration (for frontend uploads)
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
REACT_APP_CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# Application Configuration
REACT_APP_APP_NAME=TalentD Platform
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Portfolio Configuration
REACT_APP_PORTFOLIO_BASE_URL=https://portfolio.talentd.com
REACT_APP_PORTFOLIO_TEMPLATES=5

# Feature Flags
REACT_APP_ENABLE_RESUME_REVIEWER=true
REACT_APP_ENABLE_PORTFOLIO_BUILDER=true
REACT_APP_ENABLE_DSA_PROBLEMS=true

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=.pdf,.doc,.docx,.txt